{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"ElasticIPAddress1": {"Type": "AWS::EC2::EIP"}, "ElasticIPAddress2": {"Type": "AWS::EC2::EIP"}, "NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "myNetworkLoadBalancer", "Scheme": "internet-facing", "Type": "network", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "LoadBalancerAttributes": [{"Key": "deletion_protection.enabled", "Value": "false"}]}}, "Listener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "fixed-response", "FixedResponseConfig": {"StatusCode": "200", "ContentType": "text/plain", "MessageBody": "No registered targets"}}], "LoadBalancerArn": {"Ref": "NLB"}, "Port": 80, "Protocol": "TCP"}}, "NLBSubnet1EIPAssoc": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"AllocationId": {"Fn::GetAtt": ["ElasticIPAddress1", "AllocationId"]}, "NetworkInterfaceId": {"Fn::GetAtt": ["NLB", "NetworkInterfaces[0].Id"]}}}, "NLBSubnet2EIPAssoc": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"AllocationId": {"Fn::GetAtt": ["ElasticIPAddress2", "AllocationId"]}, "NetworkInterfaceId": {"Fn::GetAtt": ["NLB", "NetworkInterfaces[1].Id"]}}}}, "Parameters": {"Subnet1": {"Description": "The first subnet ID for the load balancer", "Type": "AWS::EC2::Subnet::Id"}, "Subnet2": {"Description": "The second subnet ID for the load balancer", "Type": "AWS::EC2::Subnet::Id"}}, "Outputs": {"NLBArn": {"Description": "ARN of the Network Load Balancer", "Value": {"Ref": "NLB"}}}}