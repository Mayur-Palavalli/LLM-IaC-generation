{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"MyVpc": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16", "EnableDnsSupport": "true", "EnableDnsHostnames": "true", "Tags": [{"Key": "Name", "Value": "MyVpc"}]}}, "MySubnet": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "MyVpc"}, "CidrBlock": "10.0.1.0/24", "AvailabilityZone": {"Fn::Select": ["0", {"Fn::GetAZs": ""}]}, "Tags": [{"Key": "Name", "Value": "MySubnet"}]}}, "MySecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"VpcId": {"Ref": "MyVpc"}, "GroupDescription": "Allow access to PrivateLink", "SecurityGroupIngress": [{"IpProtocol": "tcp", "FromPort": 443, "ToPort": 443, "CidrIp": "0.0.0.0/0"}], "Tags": [{"Key": "Name", "Value": "MySecurityGroup"}]}}, "MyServiceEndpoint": {"Type": "AWS::EC2::VPCEndpoint", "Properties": {"ServiceName": {"Fn::Sub": "com.amazonaws.${AWS::Region}.s3"}, "VpcId": {"Ref": "MyVpc"}, "SubnetIds": [{"Ref": "MySubnet"}], "SecurityGroupIds": [{"Ref": "MySecurityGroup"}], "VpceType": "Interface"}}, "MyPrivateLinkService": {"Type": "AWS::EC2::VPCEndpointService", "Properties": {"AcceptanceRequired": false, "NetworkLoadBalancerArns": [{"Ref": "MyNetworkLoadBalancer"}]}}, "MyPrivateLinkServiceEndpoint": {"Type": "AWS::EC2::VPCEndpoint", "Properties": {"ServiceName": {"Fn::GetAtt": ["MyPrivateLinkService", "ServiceName"]}, "VpcId": {"Ref": "MyVpc"}, "SubnetIds": [{"Ref": "MySubnet"}], "SecurityGroupIds": [{"Ref": "MySecurityGroup"}], "VpceType": "Interface"}}, "MyNetworkLoadBalancer": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Subnets": [{"Ref": "MySubnet"}], "Type": "network", "Tags": [{"Key": "Name", "Value": "MyNetworkLoadBalancer"}]}}, "MyTargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"VpcId": {"Ref": "MyVpc"}, "Port": 80, "Protocol": "HTTP", "TargetType": "instance"}}, "MyListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "MyTargetGroup"}}], "LoadBalancerArn": {"Ref": "MyNetworkLoadBalancer"}, "Port": 80, "Protocol": "HTTP"}}}, "Outputs": {"VpcId": {"Value": {"Ref": "MyVpc"}}, "SubnetId": {"Value": {"Ref": "MySubnet"}}, "SecurityGroupId": {"Value": {"Ref": "MySecurityGroup"}}, "VPCEndpointId": {"Value": {"Ref": "MyServiceEndpoint"}}, "PrivateLinkServiceId": {"Value": {"Ref": "MyPrivateLinkService"}}, "NetworkLoadBalancerId": {"Value": {"Ref": "MyNetworkLoadBalancer"}}, "TargetGroupId": {"Value": {"Ref": "MyTargetGroup"}}, "ListenerId": {"Value": {"Ref": "MyListener"}}}}