{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"VPCEndpointService": {"Type": "AWS::EC2::VPCEndpointService", "Properties": {"AcceptanceRequired": true, "NetworkLoadBalancerArns": [{"Ref": "NetworkLoadBalancer"}]}}, "NetworkLoadBalancer": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNLB", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "Scheme": "internet-facing", "Type": "network"}}, "NLBListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "TargetGroup"}}], "LoadBalancerArn": {"Ref": "NetworkLoadBalancer"}, "Port": 80, "Protocol": "TCP"}}, "TargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"Name": "MyTargetGroup", "Port": 80, "Protocol": "TCP", "VpcId": {"Ref": "VPCID"}}}}, "Parameters": {"VPCID": {"Type": "AWS::EC2::VPC::Id", "Description": "The ID of the VPC"}, "Subnet1": {"Type": "AWS::EC2::Subnet::Id", "Description": "The ID of the first subnet"}, "Subnet2": {"Type": "AWS::EC2::Subnet::Id", "Description": "The ID of the second subnet"}}, "Outputs": {"VPCEndpointServiceName": {"Description": "The name of the VPC Endpoint Service", "Value": {"Fn::GetAtt": ["VPCEndpointService", "ServiceName"]}, "Export": {"Name": {"Fn::Sub": "${AWS::StackName}-VPCEndpointServiceName"}}}}}