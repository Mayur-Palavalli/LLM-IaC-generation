{"AWSTemplateFormatVersion": "2010-09-09", "Description": "AWS CloudFormation template for creating an EC2 instance with a single ENI having multiple private and public IPs.", "Resources": {"MyEC2Instance": {"Type": "AWS::EC2::Instance", "Properties": {"ImageId": "ami-0abcdef1234567890", "InstanceType": "t2.micro", "NetworkInterfaces": [{"AssociatePublicIpAddress": true, "DeviceIndex": "0", "GroupSet": [{"Ref": "MySecurityGroup"}], "SubnetId": {"Ref": "MySubnet"}, "SecondaryPrivateIpAddressCount": 2, "PrivateIpAddresses": [{"Primary": true, "PrivateIpAddress": "10.0.0.50"}, {"Primary": false, "PrivateIpAddress": "10.0.0.51"}]}]}}, "MySecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"GroupDescription": "Enable SSH access via port 22", "VpcId": {"Ref": "MyVPC"}, "SecurityGroupIngress": [{"IpProtocol": "tcp", "FromPort": "22", "ToPort": "22", "CidrIp": "0.0.0.0/0"}]}}, "MyVPC": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16", "EnableDnsSupport": true, "EnableDnsHostnames": true}}, "MySubnet": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "MyVPC"}, "CidrBlock": "10.0.0.0/24", "MapPublicIpOnLaunch": true}}}}