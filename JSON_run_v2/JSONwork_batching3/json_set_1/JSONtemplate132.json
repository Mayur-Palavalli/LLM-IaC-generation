{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"MyDBInstance": {"Type": "AWS::RDS::DBInstance", "Properties": {"DBInstanceClass": "db.t3.micro", "Engine": "mysql", "MasterUsername": {"Ref": "DBUsername"}, "MasterUserPassword": {"Ref": "DBPassword"}, "AllocatedStorage": "20", "DBInstanceIdentifier": "mydbinstance"}, "DeletionPolicy": "Snapshot"}}, "Parameters": {"DBUsername": {"Type": "String", "Description": "The database admin account username", "MinLength": "1", "MaxLength": "16", "AllowedPattern": "[a-zA-Z][a-zA-Z0-9]*", "ConstraintDescription": "Must begin with a letter and contain only alphanumeric characters."}, "DBPassword": {"Type": "String", "Description": "The database admin account password", "MinLength": "8", "MaxLength": "41", "NoEcho": true, "ConstraintDescription": "Must contain between 8 and 41 characters."}}}