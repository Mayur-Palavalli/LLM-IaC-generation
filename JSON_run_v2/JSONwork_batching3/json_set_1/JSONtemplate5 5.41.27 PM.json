{"AWSTemplateFormatVersion": "2010-09-09", "Description": "AWS CloudFormation template to create PrivateLink infrastructure", "Resources": {"VPC": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16", "EnableDnsSupport": "true", "EnableDnsHostnames": "true"}}, "PrivateSubnet": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "VPC"}, "CidrBlock": "10.0.1.0/24", "MapPublicIpOnLaunch": "false"}}, "VPCSecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"GroupDescription": "Allow traffic within the VPC for PrivateLink", "VpcId": {"Ref": "VPC"}}}, "EndpointService": {"Type": "AWS::EC2::VPCEndpointService", "Properties": {"NetworkLoadBalancerArns": [{"Ref": "NetworkLoadBalancer"}], "AcceptanceRequired": false}}, "NetworkLoadBalancer": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"SubnetMappings": [{"SubnetId": {"Ref": "PrivateSubnet"}}], "Type": "network"}}, "VPCEndpoint": {"Type": "AWS::EC2::VPCEndpoint", "Properties": {"VpcEndpointType": "Interface", "ServiceName": {"Fn::Sub": "com.amazonaws.${AWS::Region}.ec2"}, "VpcId": {"Ref": "VPC"}, "SubnetIds": [{"Ref": "PrivateSubnet"}], "SecurityGroupIds": [{"Ref": "VPCSecurityGroup"}]}}}}