{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"MyNetworkLoadBalancer": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Subnets": [{"Ref": "SubnetA"}, {"Ref": "SubnetB"}], "Scheme": "internet-facing"}}, "EIPA": {"Type": "AWS::EC2::EIP"}, "EIPB": {"Type": "AWS::EC2::EIP"}, "NLBIpAddressA": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"IpAddressType": "ipv4", "SubnetMapping": [{"SubnetId": {"Ref": "SubnetA"}, "AllocationId": {"Fn::GetAtt": ["EIPA", "AllocationId"]}}]}, "DependsOn": ["MyNetworkLoadBalancer"]}, "NLBIpAddressB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"IpAddressType": "ipv4", "SubnetMapping": [{"SubnetId": {"Ref": "SubnetB"}, "AllocationId": {"Fn::GetAtt": ["EIPB", "AllocationId"]}}]}, "DependsOn": ["MyNetworkLoadBalancer"]}, "MyTargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"Name": "MyTargetGroup", "Port": 80, "Protocol": "TCP", "VpcId": {"Ref": "VpcId"}, "TargetType": "instance", "HealthCheckProtocol": "TCP", "HealthCheckPort": "80", "HealthCheckEnabled": true}}, "MyListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "MyTargetGroup"}}], "LoadBalancerArn": {"Ref": "MyNetworkLoadBalancer"}, "Port": 80, "Protocol": "TCP"}}}, "Parameters": {"SubnetA": {"Type": "AWS::EC2::Subnet::Id", "Description": "Subnet A ID"}, "SubnetB": {"Type": "AWS::EC2::Subnet::Id", "Description": "Subnet B ID"}, "VpcId": {"Type": "AWS::EC2::VPC::Id", "Description": "VPC ID"}}}