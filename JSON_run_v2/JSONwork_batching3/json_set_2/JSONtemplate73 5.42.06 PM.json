{"AWSTemplateFormatVersion": "2010-09-09", "Description": "AWS CloudFormation template for a Multi-AZ EFS with Auto-Mount", "Resources": {"FileSystem": {"Type": "AWS::EFS::FileSystem", "Properties": {"Encrypted": true, "PerformanceMode": "generalPurpose", "ThroughputMode": "bursting", "LifecyclePolicies": [{"TransitionToIA": "AFTER_30_DAYS"}]}}, "MountTarget1": {"Type": "AWS::EFS::MountTarget", "Properties": {"FileSystemId": {"Ref": "FileSystem"}, "SubnetId": "subnet-xxxxxxxx", "SecurityGroups": ["sg-xxxxxxxx"]}}, "MountTarget2": {"Type": "AWS::EFS::MountTarget", "Properties": {"FileSystemId": {"Ref": "FileSystem"}, "SubnetId": "subnet-yyyyyyyy", "SecurityGroups": ["sg-xxxxxxxx"]}}}, "Outputs": {"FileSystemId": {"Description": "The ID of the EFS file system", "Value": {"Ref": "FileSystem"}}}}