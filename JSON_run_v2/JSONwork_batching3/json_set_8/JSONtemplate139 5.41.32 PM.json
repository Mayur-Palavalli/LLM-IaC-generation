{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"MyDBParameterGroup": {"Type": "AWS::RDS::DBParameterGroup", "Properties": {"Description": "Custom parameter group for MyDBInstance", "Family": "mysql5.7", "Parameters": {"max_connections": "150"}}}, "MyDBInstance": {"Type": "AWS::RDS::DBInstance", "Properties": {"DBInstanceClass": "db.t2.micro", "AllocatedStorage": "20", "DBName": "MyDatabase", "Engine": "mysql", "MasterUsername": "adminuser", "MasterUserPassword": "{{resolve:ssm-secure:DBPassword:1}}", "DBParameterGroupName": {"Ref": "MyDBParameterGroup"}}}}, "Outputs": {"DBInstanceEndpoint": {"Description": "The endpoint of the RDS instance", "Value": {"Fn::GetAtt": ["MyDBInstance", "Endpoint.Address"]}}}}