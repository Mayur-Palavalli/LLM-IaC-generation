{"AWSTemplateFormatVersion": "2010-09-09", "Description": "AWS CloudFormation template to deploy a VPC with a pair of private subnets spread across two Availability Zones and a VPC Endpoint for CloudFormation.", "Resources": {"VPC": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16", "EnableDnsSupport": true, "EnableDnsHostnames": true, "Tags": [{"Key": "Name", "Value": "MyVPC"}]}}, "SubnetPrivate1": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "VPC"}, "CidrBlock": "10.0.1.0/24", "AvailabilityZone": {"Fn::Select": [0, {"Fn::GetAZs": {"Ref": "AWS::Region"}}]}, "Tags": [{"Key": "Name", "Value": "PrivateSubnet1"}]}}, "SubnetPrivate2": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "VPC"}, "CidrBlock": "10.0.2.0/24", "AvailabilityZone": {"Fn::Select": [1, {"Fn::GetAZs": {"Ref": "AWS::Region"}}]}, "Tags": [{"Key": "Name", "Value": "PrivateSubnet2"}]}}, "VPCEndpointCloudFormation": {"Type": "AWS::EC2::VPCEndpoint", "Properties": {"ServiceName": {"Fn::Sub": "com.amazonaws.${AWS::Region}.cloudformation"}, "VpcId": {"Ref": "VPC"}, "SubnetIds": [{"Ref": "SubnetPrivate1"}, {"Ref": "SubnetPrivate2"}], "VpcEndpointType": "Interface", "PrivateDnsEnabled": true, "SecurityGroupIds": [{"Ref": "EndpointSecurityGroup"}]}}, "EndpointSecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"GroupDescription": "Security group for VPC Endpoints in the VPC", "VpcId": {"Ref": "VPC"}, "SecurityGroupIngress": [{"IpProtocol": "tcp", "FromPort": 443, "ToPort": 443, "CidrIp": "10.0.0.0/16"}], "Tags": [{"Key": "Name", "Value": "EndpointSecurityGroup"}]}}}, "Outputs": {"VPCId": {"Description": "VPC Id", "Value": {"Ref": "VPC"}}, "PrivateSubnet1": {"Description": "Private Subnet 1 Id", "Value": {"Ref": "SubnetPrivate1"}}, "PrivateSubnet2": {"Description": "Private Subnet 2 Id", "Value": {"Ref": "SubnetPrivate2"}}, "VPCEndpointCloudFormationId": {"Description": "VPC Endpoint CloudFormation Id", "Value": {"Ref": "VPCEndpointCloudFormation"}}}}