{"AWSTemplateFormatVersion": "2010-09-09", "Description": "AWS CloudFormation template to create a Network Load Balancer (NLB) across 2 Availability Zones (AZs) listening on TCP port 80 with Elastic IPs.", "Resources": {"EIP1": {"Type": "AWS::EC2::EIP", "Properties": {"Domain": "vpc"}}, "EIP2": {"Type": "AWS::EC2::EIP", "Properties": {"Domain": "vpc"}}, "NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNLB", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "Scheme": "internet-facing", "Type": "network", "LoadBalancerAttributes": [{"Key": "deletion_protection.enabled", "Value": "false"}]}}, "Listener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "TargetGroup"}}], "LoadBalancerArn": {"Ref": "NLB"}, "Port": 80, "Protocol": "TCP"}}, "TargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"HealthCheckProtocol": "TCP", "Port": 80, "Protocol": "TCP", "VpcId": {"Ref": "VPCId"}, "TargetType": "instance"}}}, "Parameters": {"Subnet1": {"Type": "AWS::EC2::Subnet::Id", "Description": "Subnet ID for the first Availability Zone."}, "Subnet2": {"Type": "AWS::EC2::Subnet::Id", "Description": "Subnet ID for the second Availability Zone."}, "VPCId": {"Type": "AWS::EC2::VPC::Id", "Description": "VPC ID where the NLB and other resources will be deployed."}}, "Outputs": {"NetworkLoadBalancer": {"Description": "The ARN of the NLB", "Value": {"Ref": "NLB"}}, "ListenerARN": {"Description": "The ARN of the Listener", "Value": {"Ref": "Listener"}}, "TargetGroupARN": {"Description": "The ARN of the Target Group", "Value": {"Ref": "TargetGroup"}}}}