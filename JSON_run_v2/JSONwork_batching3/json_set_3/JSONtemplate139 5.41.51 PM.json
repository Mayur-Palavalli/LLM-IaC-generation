{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"MyDBInstance": {"Type": "AWS::RDS::DBInstance", "Properties": {"DBInstanceClass": "db.t2.micro", "AllocatedStorage": "20", "DBName": "MyDatabase", "Engine": "mysql", "MasterUsername": "adminuser", "MasterUserPassword": {"Ref": "DBPassword"}, "DBParameterGroupName": {"Ref": "MyDBParameterGroup"}}}, "MyDBParameterGroup": {"Type": "AWS::RDS::DBParameterGroup", "Properties": {"Description": "Custom parameter group for MyDBInstance", "Family": "mysql5.7", "Parameters": {"max_connections": "150"}}}}, "Parameters": {"DBPassword": {"Type": "String", "Description": "The database admin password", "NoEcho": "true"}}, "Outputs": {"DBInstanceEndpoint": {"Description": "The endpoint of the RDS instance", "Value": {"Fn::GetAtt": ["MyDBInstance", "Endpoint.Address"]}}}}