{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Scheme": "internet-facing", "LoadBalancerAttributes": [], "Subnets": [{"Ref": "SubnetA"}, {"Ref": "SubnetB"}]}}, "ElasticIPAddressA": {"Type": "AWS::EC2::EIP"}, "ElasticIPAddressB": {"Type": "AWS::EC2::EIP"}, "LBListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"LoadBalancerArn": {"Ref": "NLB"}, "Port": 80, "Protocol": "TCP", "DefaultActions": [{"Type": "fixed-response", "FixedResponseConfig": {"StatusCode": "200", "ContentType": "text/plain", "MessageBody": "No targets registered."}}]}}}, "Outputs": {"NLBDNSName": {"Description": "DNS Name of the Network Load Balancer", "Value": {"Fn::GetAtt": ["NLB", "DNSName"]}}}, "Parameters": {"SubnetA": {"Type": "AWS::EC2::Subnet::Id", "Description": "The subnet ID of the first subnet."}, "SubnetB": {"Type": "AWS::EC2::Subnet::Id", "Description": "The subnet ID of the second subnet."}}}