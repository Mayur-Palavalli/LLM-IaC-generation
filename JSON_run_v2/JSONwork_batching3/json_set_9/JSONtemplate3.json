{"AWSTemplateFormatVersion": "2010-09-09", "Description": "AWS CloudFormation Template to create PrivateLink infrastructure", "Resources": {"VPC": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16", "EnableDnsSupport": true, "EnableDnsHostnames": true, "Tags": [{"Key": "Name", "Value": "PrivateLinkVPC"}]}}, "Subnet": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "VPC"}, "CidrBlock": "10.0.1.0/24", "AvailabilityZone": {"Fn::Select": ["0", {"Fn::GetAZs": ""}]}, "Tags": [{"Key": "Name", "Value": "PrivateLinkSubnet"}]}}, "SecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"GroupDescription": "Allow PrivateLink traffic", "VpcId": {"Ref": "VPC"}, "SecurityGroupIngress": [{"IpProtocol": "tcp", "FromPort": 443, "ToPort": 443, "CidrIp": "0.0.0.0/0"}], "Tags": [{"Key": "Name", "Value": "PrivateLinkSecurityGroup"}]}}, "ServiceEndpoint": {"Type": "AWS::EC2::VPCEndpoint", "Properties": {"VpcId": {"Ref": "VPC"}, "ServiceName": "<YOUR_SERVICE_NAME>", "PrivateDnsEnabled": true, "SubnetIds": [{"Ref": "Subnet"}], "VpcEndpointType": "Interface", "SecurityGroupIds": [{"Ref": "SecurityGroup"}]}}}, "Outputs": {"VPCEndpointId": {"Description": "ID of the VPC Endpoint", "Value": {"Ref": "ServiceEndpoint"}}}}