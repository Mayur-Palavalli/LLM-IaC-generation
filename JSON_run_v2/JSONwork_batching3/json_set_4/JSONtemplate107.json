{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"EIP1": {"Type": "AWS::EC2::EIP"}, "EIP2": {"Type": "AWS::EC2::EIP"}, "NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "LoadBalancerAttributes": [{"Key": "deletion_protection.enabled", "Value": "false"}]}}, "NLBEIP1Assoc": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"AllocationId": {"Fn::GetAtt": ["EIP1", "AllocationId"]}, "NetworkInterfaceId": {"Fn::Select": ["0", {"Fn::GetAtt": ["NLB", "LoadBalancerId"]}]}}}, "NLBEIP2Assoc": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"AllocationId": {"Fn::GetAtt": ["EIP2", "AllocationId"]}, "NetworkInterfaceId": {"Fn::Select": ["1", {"Fn::GetAtt": ["NLB", "LoadBalancerId"]}]}}}, "TargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"Name": "MyTargetGroup", "Protocol": "TCP", "Port": 80, "VpcId": {"Ref": "VPC"}, "TargetType": "instance", "HealthCheckProtocol": "TCP", "HealthCheckPort": "80", "HealthCheckIntervalSeconds": 30, "HealthCheckTimeoutSeconds": 10, "HealthyThresholdCount": 3, "UnhealthyThresholdCount": 3}}, "Listener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"LoadBalancerArn": {"Ref": "NLB"}, "Port": 80, "Protocol": "TCP", "DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "TargetGroup"}}]}}}, "Parameters": {"VPC": {"Type": "AWS::EC2::VPC::Id", "Description": "The VPC ID"}, "Subnet1": {"Type": "AWS::EC2::Subnet::Id", "Description": "The first subnet ID"}, "Subnet2": {"Type": "AWS::EC2::Subnet::Id", "Description": "The second subnet ID"}}}