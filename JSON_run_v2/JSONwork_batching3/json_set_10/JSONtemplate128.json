{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"MyDBSubnetGroup": {"Type": "AWS::RDS::DBSubnetGroup", "Properties": {"DBSubnetGroupDescription": "Subnet group for RDS instance", "SubnetIds": ["subnet-1a2b3c4d", "subnet-4d3c2b1a"]}}, "MyDBInstance": {"Type": "AWS::RDS::DBInstance", "Properties": {"AllocatedStorage": "20", "DBInstanceClass": "db.t3.micro", "Engine": "mysql", "MasterUsername": "admin", "MasterUserPassword": "ReplaceWithYourSecurePassword", "DBSubnetGroupName": {"Ref": "MyDBSubnetGroup"}, "MultiAZ": true, "PubliclyAccessible": false, "StorageType": "gp2", "VPCSecurityGroups": ["sg-12345678"]}}, "MyReadReplica": {"Type": "AWS::RDS::DBInstance", "Properties": {"SourceDBInstanceIdentifier": {"Ref": "MyDBInstance"}, "DBInstanceClass": "db.t3.micro", "PubliclyAccessible": false, "StorageType": "gp2"}}}, "Parameters": {}, "Outputs": {"PrimaryDBInstanceIdentifier": {"Description": "Identifier for the primary DB instance", "Value": {"Ref": "MyDBInstance"}}, "ReadReplicaIdentifier": {"Description": "Identifier for the read replica", "Value": {"Ref": "MyReadReplica"}}}}