{"AWSTemplateFormatVersion": "2010-09-09", "Description": "AWS CloudFormation Template for a load balanced, Auto Scaled sample website with time-based scheduled actions.", "Resources": {"SampleLoadBalancer": {"Type": "AWS::ElasticLoadBalancing::LoadBalancer", "Properties": {"Listeners": [{"LoadBalancerPort": "80", "InstancePort": "80", "Protocol": "HTTP"}], "HealthCheck": {"Target": "HTTP:80/", "HealthyThreshold": "3", "UnhealthyThreshold": "5", "Interval": "30", "Timeout": "5"}, "AvailabilityZones": {"Fn::GetAZs": ""}}}, "SampleLaunchConfiguration": {"Type": "AWS::AutoScaling::LaunchConfiguration", "Properties": {"ImageId": "ami-0abcdef1234567890", "InstanceType": "t2.micro"}}, "SampleAutoScalingGroup": {"Type": "AWS::AutoScaling::AutoScalingGroup", "Properties": {"VPCZoneIdentifier": ["subnet-12345678"], "LaunchConfigurationName": {"Ref": "SampleLaunchConfiguration"}, "MinSize": "1", "MaxSize": "5", "DesiredCapacity": "3", "Tags": [{"Key": "Name", "Value": "SampleAutoScalingGroup", "PropagateAtLaunch": "true"}]}}, "ScaleUpPolicy": {"Type": "AWS::AutoScaling::ScalingPolicy", "Properties": {"AutoScalingGroupName": {"Ref": "SampleAutoScalingGroup"}, "PolicyType": "TargetTrackingScaling", "TargetTrackingConfiguration": {"PredefinedMetricSpecification": {"PredefinedMetricType": "ASGAverageCPUUtilization"}, "TargetValue": 50.0}}}, "ScheduledAction": {"Type": "AWS::AutoScaling::ScheduledAction", "Properties": {"AutoScalingGroupName": {"Ref": "SampleAutoScalingGroup"}, "DesiredCapacity": "4", "Recurrence": "0 18 * * *", "EndTime": "2023-12-31T23:59:59Z", "StartTime": "2023-01-01T00:00:00Z"}}}, "Outputs": {"LoadBalancerDNSName": {"Description": "DNS name of the load balancer", "Value": {"Fn::GetAtt": ["SampleLoadBalancer", "DNSName"]}}}}