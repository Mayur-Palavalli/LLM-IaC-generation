{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"MyVPCEndpointService": {"Type": "AWS::EC2::VPCEndpointService", "Properties": {"AcceptanceRequired": true, "NetworkLoadBalancerArns": [{"Ref": "MyNetworkLoadBalancer"}]}}, "MyVPCEndpoint": {"Type": "AWS::EC2::VPCEndpoint", "Properties": {"VpcId": {"Ref": "MyVPC"}, "VpcEndpointType": "Interface", "PrivateDnsEnabled": true, "SecurityGroupIds": [{"Ref": "MySecurityGroup"}], "SubnetIds": [{"Ref": "MySubnet1"}, {"Ref": "MySubnet2"}], "ServiceName": {"Fn::GetAtt": ["MyVPCEndpointService", "ServiceName"]}}}, "MySecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"GroupDescription": "Security group for PrivateLink", "VpcId": {"Ref": "MyVPC"}}}, "MyVPC": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16"}}, "MySubnet1": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "MyVPC"}, "CidrBlock": "10.0.1.0/24", "AvailabilityZone": {"Fn::Select": [0, {"Fn::GetAZs": ""}]}}}, "MySubnet2": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "MyVPC"}, "CidrBlock": "10.0.2.0/24", "AvailabilityZone": {"Fn::Select": [1, {"Fn::GetAZs": ""}]}}}, "MyNetworkLoadBalancer": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Type": "network", "Subnets": [{"Ref": "MySubnet1"}, {"Ref": "MySubnet2"}]}}}, "Outputs": {"VPCEndpointServiceName": {"Description": "The name of the VPC Endpoint Service", "Value": {"Fn::GetAtt": ["MyVPCEndpointService", "ServiceName"]}}, "VPCEndpointId": {"Description": "The ID of the VPC Endpoint", "Value": {"Ref": "MyVPCEndpoint"}}}}