{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "Scheme": "internet-facing", "LoadBalancerAttributes": [], "IpAddressType": "ipv4"}}, "NLBListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "NLBTargetGroup"}}], "LoadBalancerArn": {"Ref": "NLB"}, "Port": 80, "Protocol": "TCP"}, "DependsOn": "NLB"}, "NLBTargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"Name": "MyTargetGroup", "Port": 80, "Protocol": "TCP", "VpcId": {"Ref": "VpcId"}, "TargetType": "instance", "HealthCheckProtocol": "TCP", "HealthCheckPort": "80", "HealthyThresholdCount": 3, "UnhealthyThresholdCount": 3}}, "EIP1": {"Type": "AWS::EC2::EIP", "Properties": {"Domain": "vpc"}}, "EIP2": {"Type": "AWS::EC2::EIP", "Properties": {"Domain": "vpc"}}, "ENI1": {"Type": "AWS::EC2::NetworkInterface", "Properties": {"SubnetId": {"Ref": "Subnet1"}}}, "ENI2": {"Type": "AWS::EC2::NetworkInterface", "Properties": {"SubnetId": {"Ref": "Subnet2"}}}, "ENIAttachment1": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"EIP": {"Ref": "EIP1"}, "NetworkInterfaceId": {"Ref": "ENI1"}}}, "ENIAttachment2": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"EIP": {"Ref": "EIP2"}, "NetworkInterfaceId": {"Ref": "ENI2"}}}}, "Parameters": {"Subnet1": {"Type": "String", "Description": "Subnet ID for the first subnet"}, "Subnet2": {"Type": "String", "Description": "Subnet ID for the second subnet"}, "VpcId": {"Type": "String", "Description": "VPC ID"}}, "Outputs": {"NLBDNSName": {"Description": "DNS name of the Network Load Balancer", "Value": {"Fn::GetAtt": ["NLB", "DNSName"]}}}}