{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "Scheme": "internet-facing", "LoadBalancerAttributes": [{"Key": "deletion_protection.enabled", "Value": "false"}]}}, "MyTargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"VpcId": {"Ref": "VpcId"}, "Port": 80, "Protocol": "TCP", "TargetType": "instance"}}, "EIP1": {"Type": "AWS::EC2::EIP"}, "EIP2": {"Type": "AWS::EC2::EIP"}, "NLBENI1": {"Type": "AWS::EC2::NetworkInterface", "Properties": {"SubnetId": {"Ref": "Subnet1"}, "Description": "NLB ENI 1", "PrivateIpAddress": {"Fn::GetAtt": ["EIP1", "AllocationId"]}}}, "NLBENI2": {"Type": "AWS::EC2::NetworkInterface", "Properties": {"SubnetId": {"Ref": "Subnet2"}, "Description": "NLB ENI 2", "PrivateIpAddress": {"Fn::GetAtt": ["EIP2", "AllocationId"]}}}}, "Outputs": {"NLBName": {"Description": "The name of the Network Load Balancer", "Value": {"Ref": "NLB"}}, "NLBArn": {"Description": "The ARN of the Network Load Balancer", "Value": {"Fn::GetAtt": ["NLB", "LoadBalancerArn"]}}}, "Parameters": {"VpcId": {"Type": "String", "Description": "The VPC ID where the NLB will be deployed"}, "Subnet1": {"Type": "String", "Description": "A subnet ID for the first AZ"}, "Subnet2": {"Type": "String", "Description": "A subnet ID for the second AZ"}}}