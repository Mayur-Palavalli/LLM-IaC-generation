{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"ElasticLoadBalancerEIP1": {"Type": "AWS::EC2::EIP"}, "ElasticLoadBalancerEIP2": {"Type": "AWS::EC2::EIP"}, "ElasticLoadBalancer": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Subnets": [{"Ref": "SubnetA"}, {"Ref": "SubnetB"}], "Scheme": "internet-facing", "LoadBalancerAttributes": [], "IpAddressType": "ipv4"}}, "ElasticLoadBalancerListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "MyTargetGroup"}}], "LoadBalancerArn": {"Ref": "ElasticLoadBalancer"}, "Port": 80, "Protocol": "TCP"}}, "MyTargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"Port": 80, "Protocol": "TCP", "VpcId": {"Ref": "VPC"}, "HealthCheckProtocol": "TCP", "HealthCheckPort": "traffic-port", "TargetType": "instance"}}}, "Parameters": {"SubnetA": {"Type": "AWS::EC2::Subnet::Id", "Description": "Subnet ID for the first AZ"}, "SubnetB": {"Type": "AWS::EC2::Subnet::Id", "Description": "Subnet ID for the second AZ"}, "VPC": {"Type": "AWS::EC2::VPC::Id", "Description": "VPC ID"}}, "Outputs": {"LoadBalancerDNSName": {"Description": "DNS Name of the Load Balancer", "Value": {"Fn::GetAtt": ["ElasticLoadBalancer", "DNSName"]}}}}