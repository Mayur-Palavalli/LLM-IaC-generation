{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"ElasticIP1": {"Type": "AWS::EC2::EIP"}, "ElasticIP2": {"Type": "AWS::EC2::EIP"}, "NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Scheme": "internet-facing", "LoadBalancerAttributes": [{"Key": "load_balancing.cross_zone.enabled", "Value": "true"}], "SubnetMappings": [{"SubnetId": {"Ref": "Subnet1"}, "AllocationId": {"Fn::GetAtt": ["ElasticIP1", "AllocationId"]}}, {"SubnetId": {"Ref": "Subnet2"}, "AllocationId": {"Fn::GetAtt": ["ElasticIP2", "AllocationId"]}}]}}, "Listener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "TargetGroup"}}], "LoadBalancerArn": {"Ref": "NLB"}, "Port": 80, "Protocol": "TCP"}}, "TargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"HealthCheckProtocol": "TCP", "Port": 80, "Protocol": "TCP", "VpcId": {"Ref": "VpcId"}}}}, "Parameters": {"Subnet1": {"Type": "AWS::EC2::Subnet::Id", "Description": "The subnet ID for the first availability zone"}, "Subnet2": {"Type": "AWS::EC2::Subnet::Id", "Description": "The subnet ID for the second availability zone"}, "VpcId": {"Type": "AWS::EC2::VPC::Id", "Description": "The VPC ID where NLB and target group will be created"}}, "Outputs": {"LoadBalancerDNSName": {"Description": "The DNS name of the load balancer", "Value": {"Fn::GetAtt": ["NLB", "DNSName"]}}, "TargetGroupArn": {"Description": "The ARN of the target group", "Value": {"Ref": "TargetGroup"}}}}