{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"VPC": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16", "EnableDnsSupport": true, "EnableDnsHostnames": true, "Tags": [{"Key": "Name", "Value": "PrivateLink-VPC"}]}}, "Subnet1": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "VPC"}, "CidrBlock": "10.0.1.0/24", "MapPublicIpOnLaunch": false, "Tags": [{"Key": "Name", "Value": "PrivateLink-Subnet1"}]}}, "Subnet2": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "VPC"}, "CidrBlock": "10.0.2.0/24", "MapPublicIpOnLaunch": false, "Tags": [{"Key": "Name", "Value": "PrivateLink-Subnet2"}]}}, "NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Type": "network", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "Tags": [{"Key": "Name", "Value": "PrivateLink-NLB"}]}}, "NLBListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"LoadBalancerArn": {"Ref": "NLB"}, "Port": 443, "Protocol": "TCP", "DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "NLBTargetGroup"}}]}}, "NLBTargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"VpcId": {"Ref": "VPC"}, "Protocol": "TCP", "Port": 443, "HealthCheckProtocol": "TCP", "HealthCheckPort": "traffic-port", "TargetType": "ip"}}, "VPCServiceEndpoint": {"Type": "AWS::EC2::VPCEndpointService", "Properties": {"NetworkLoadBalancerArns": [{"Ref": "NLB"}], "AcceptanceRequired": true}}}, "Outputs": {"ServiceEndpointName": {"Description": "Name of the VPC Endpoint Service", "Value": {"Fn::GetAtt": ["VPCServiceEndpoint", "ServiceName"]}}}}