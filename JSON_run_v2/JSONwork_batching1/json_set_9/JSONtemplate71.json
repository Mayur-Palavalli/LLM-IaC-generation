{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"EFSFileSystem": {"Type": "AWS::EFS::FileSystem", "Properties": {"Encrypted": true, "FileSystemTags": [{"Key": "Name", "Value": "MyEFS"}], "PerformanceMode": "generalPurpose"}}, "MountTarget1": {"Type": "AWS::EFS::MountTarget", "Properties": {"FileSystemId": {"Ref": "EFSFileSystem"}, "SecurityGroups": [{"Ref": "EFSSecurityGroup"}], "SubnetId": {"Ref": "Subnet1"}}, "DependsOn": "EFSSecurityGroup"}, "MountTarget2": {"Type": "AWS::EFS::MountTarget", "Properties": {"FileSystemId": {"Ref": "EFSFileSystem"}, "SecurityGroups": [{"Ref": "EFSSecurityGroup"}], "SubnetId": {"Ref": "Subnet2"}}, "DependsOn": "EFSSecurityGroup"}, "MountTarget3": {"Type": "AWS::EFS::MountTarget", "Properties": {"FileSystemId": {"Ref": "EFSFileSystem"}, "SecurityGroups": [{"Ref": "EFSSecurityGroup"}], "SubnetId": {"Ref": "Subnet3"}}, "DependsOn": "EFSSecurityGroup"}, "EFSSecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"GroupDescription": "Allow access to EFS", "SecurityGroupIngress": [{"IpProtocol": "tcp", "FromPort": 2049, "ToPort": 2049, "CidrIp": "0.0.0.0/0"}], "VpcId": {"Ref": "VPC"}}}}, "Outputs": {"FileSystemId": {"Description": "The ID of the EFS file system", "Value": {"Ref": "EFSFileSystem"}}}, "Parameters": {"VPC": {"Type": "AWS::EC2::VPC::Id", "Description": "The VPC ID"}, "Subnet1": {"Type": "AWS::EC2::Subnet::Id", "Description": "The ID of the first subnet"}, "Subnet2": {"Type": "AWS::EC2::Subnet::Id", "Description": "The ID of the second subnet"}, "Subnet3": {"Type": "AWS::EC2::Subnet::Id", "Description": "The ID of the third subnet"}}}