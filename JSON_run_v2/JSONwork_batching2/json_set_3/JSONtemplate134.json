{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"MyDBInstance": {"Type": "AWS::RDS::DBInstance", "Properties": {"DBInstanceClass": "db.t2.micro", "Engine": "mysql", "MasterUsername": "masteruser", "MasterUserPassword": {"Fn::Sub": "{{resolve:secretsmanager:DBMasterUserPassword:SecretString:password}}"}, "AllocatedStorage": "20", "BackupRetentionPeriod": 7, "DBName": "mydatabase"}, "DeletionPolicy": "Snapshot", "UpdateReplacePolicy": "Snapshot"}}, "Parameters": {"DBMasterUserPassword": {"NoEcho": true, "Description": "The database admin account password", "Type": "String", "MinLength": 8, "MaxLength": 41, "AllowedPattern": "^[a-zA-Z0-9@#$%^&+=]*$", "ConstraintDescription": "The password must be between 8 and 41 characters and contain only alphanumeric characters and @#$%^&+="}}}