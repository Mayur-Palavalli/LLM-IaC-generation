{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"ElasticIP1": {"Type": "AWS::EC2::EIP"}, "ElasticIP2": {"Type": "AWS::EC2::EIP"}, "NetworkLoadBalancer": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "LoadBalancerAttributes": [{"Key": "deletion_protection.enabled", "Value": "false"}], "Scheme": "internet-facing", "IpAddressType": "ipv4"}}, "LoadBalancerListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "fixed-response", "FixedResponseConfig": {"StatusCode": "200", "ContentType": "text/plain", "MessageBody": "OK"}}], "LoadBalancerArn": {"Ref": "NetworkLoadBalancer"}, "Port": 80, "Protocol": "TCP"}}, "LoadBalancerEIPAssociation1": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"AllocationId": {"Fn::GetAtt": ["ElasticIP1", "AllocationId"]}, "NetworkInterfaceId": {"Fn::Select": ["0", {"Fn::Split": ["/", {"Fn::Select": ["1", {"Fn::Split": ["/network-interface/", {"Fn::GetAtt": ["NetworkLoadBalancer", "DNSName"]}]}]}]}]}}}, "LoadBalancerEIPAssociation2": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"AllocationId": {"Fn::GetAtt": ["ElasticIP2", "AllocationId"]}, "NetworkInterfaceId": {"Fn::Select": ["1", {"Fn::Split": ["/", {"Fn::Select": ["1", {"Fn::Split": ["/network-interface/", {"Fn::GetAtt": ["NetworkLoadBalancer", "DNSName"]}]}]}]}]}}}}, "Outputs": {"NetworkLoadBalancerDNSName": {"Description": "DNS name for the Network Load Balancer", "Value": {"Fn::GetAtt": ["NetworkLoadBalancer", "DNSName"]}}}, "Parameters": {"Subnet1": {"Type": "AWS::EC2::Subnet::Id", "Description": "The first subnet ID for the Network Load Balancer"}, "Subnet2": {"Type": "AWS::EC2::Subnet::Id", "Description": "The second subnet ID for the Network Load Balancer"}}}