{"AWSTemplateFormatVersion": "2010-09-09", "Description": "AWS CloudFormation template to create a VPC with private subnets and a VPC Endpoint for CloudFormation", "Resources": {"MyVPC": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16", "EnableDnsSupport": true, "EnableDnsHostnames": true, "Tags": [{"Key": "Name", "Value": "MyVPC"}]}}, "MySubnet1": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "MyVPC"}, "CidrBlock": "10.0.1.0/24", "AvailabilityZone": {"Fn::Select": ["0", {"Fn::GetAZs": ""}]}, "Tags": [{"Key": "Name", "Value": "MySubnet1"}]}}, "MySubnet2": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "MyVPC"}, "CidrBlock": "10.0.2.0/24", "AvailabilityZone": {"Fn::Select": ["1", {"Fn::GetAZs": ""}]}, "Tags": [{"Key": "Name", "Value": "MySubnet2"}]}}, "VPCEndpoint": {"Type": "AWS::EC2::VPCEndpoint", "Properties": {"VpcId": {"Ref": "MyVPC"}, "ServiceName": {"Fn::Sub": "com.amazonaws.${AWS::Region}.cloudformation"}, "VpcEndpointType": "Interface", "SubnetIds": [{"Ref": "MySubnet1"}, {"Ref": "MySubnet2"}], "PrivateDnsEnabled": true, "SecurityGroupIds": [{"Ref": "MySecurityGroup"}]}}, "MySecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"GroupName": "cfn-signal-access", "GroupDescription": "Enable communication with CloudFormation endpoint", "VpcId": {"Ref": "MyVPC"}, "SecurityGroupIngress": [{"IpProtocol": "tcp", "FromPort": 443, "ToPort": 443, "CidrIp": "0.0.0.0/0"}], "Tags": [{"Key": "Name", "Value": "cfn-signal-access"}]}}}, "Outputs": {"VPCId": {"Description": "VPC Id", "Value": {"Ref": "MyVPC"}}, "Subnet1Id": {"Description": "Subnet 1 Id", "Value": {"Ref": "MySubnet1"}}, "Subnet2Id": {"Description": "Subnet 2 Id", "Value": {"Ref": "MySubnet2"}}, "VPCEndpointId": {"Description": "VPC Endpoint Id", "Value": {"Ref": "VPCEndpoint"}}}}