{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"EIP1": {"Type": "AWS::EC2::EIP"}, "EIP2": {"Type": "AWS::EC2::EIP"}, "NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Type": "network", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "LoadBalancerAttributes": [{"Key": "load_balancer.eip_address.allocation_ids", "Value": {"Fn::Join": [",", [{"Fn::GetAtt": ["EIP1", "AllocationId"]}, {"Fn::GetAtt": ["EIP2", "AllocationId"]}]]}}]}}, "NLBListener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"LoadBalancerArn": {"Ref": "NLB"}, "Port": 80, "Protocol": "TCP", "DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "TargetGroup"}}]}}, "TargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"VpcId": {"Ref": "VPC"}, "Port": 80, "Protocol": "TCP", "HealthCheckProtocol": "TCP", "Targets": []}}}, "Parameters": {"Subnet1": {"Type": "AWS::EC2::Subnet::Id", "Description": "The first subnet ID"}, "Subnet2": {"Type": "AWS::EC2::Subnet::Id", "Description": "The second subnet ID"}, "VPC": {"Type": "AWS::EC2::VPC::Id", "Description": "The VPC ID"}}}