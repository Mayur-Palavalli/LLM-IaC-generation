{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"ElasticIPAddress1": {"Type": "AWS::EC2::EIP"}, "ElasticIPAddress2": {"Type": "AWS::EC2::EIP"}, "NetworkLoadBalancer": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNetworkLoadBalancer", "Type": "network", "Subnets": [{"Ref": "SubnetA"}, {"Ref": "SubnetB"}], "LoadBalancerAttributes": [{"Key": "idle_timeout.timeout_seconds", "Value": "3500"}], "Scheme": "internet-facing"}}, "ElasticIPAddressAssociation1": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"AllocationId": {"Fn::GetAtt": ["ElasticIPAddress1", "AllocationId"]}, "NetworkInterfaceId": {"Fn::GetAtt": ["NetworkLoadBalancer", "NetworkInterfaceIds[0]"]}}}, "ElasticIPAddressAssociation2": {"Type": "AWS::EC2::EIPAssociation", "Properties": {"AllocationId": {"Fn::GetAtt": ["ElasticIPAddress2", "AllocationId"]}, "NetworkInterfaceId": {"Fn::GetAtt": ["NetworkLoadBalancer", "NetworkInterfaceIds[1]"]}}}, "TargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"Name": "MyTargetGroup", "Port": 80, "Protocol": "TCP", "VpcId": {"Ref": "VpcId"}, "HealthCheckProtocol": "TCP", "TargetType": "instance"}}, "Listener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"LoadBalancerArn": {"Ref": "NetworkLoadBalancer"}, "Port": 80, "Protocol": "TCP", "DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "TargetGroup"}}]}}}, "Parameters": {"SubnetA": {"Type": "AWS::EC2::Subnet::Id", "Description": "Subnet ID for the first Availability Zone"}, "SubnetB": {"Type": "AWS::EC2::Subnet::Id", "Description": "Subnet ID for the second Availability Zone"}, "VpcId": {"Type": "AWS::EC2::VPC::Id", "Description": "VPC ID for the load balancer and the target group"}}}