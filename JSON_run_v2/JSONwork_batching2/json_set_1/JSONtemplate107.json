{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"EIP1": {"Type": "AWS::EC2::EIP"}, "EIP2": {"Type": "AWS::EC2::EIP"}, "NLB": {"Type": "AWS::ElasticLoadBalancingV2::LoadBalancer", "Properties": {"Name": "MyNLB", "Scheme": "internet-facing", "Type": "network", "Subnets": [{"Ref": "Subnet1"}, {"Ref": "Subnet2"}], "LoadBalancerAttributes": [{"Key": "load_balancing.cross_zone.enabled", "Value": "true"}], "SubnetMappings": [{"SubnetId": {"Ref": "Subnet1"}, "AllocationId": {"Fn::GetAtt": ["EIP1", "AllocationId"]}}, {"SubnetId": {"Ref": "Subnet2"}, "AllocationId": {"Fn::GetAtt": ["EIP2", "AllocationId"]}}]}}, "TargetGroup": {"Type": "AWS::ElasticLoadBalancingV2::TargetGroup", "Properties": {"Name": "MyTargetGroup", "Port": 80, "Protocol": "TCP", "VpcId": {"Ref": "VPC"}, "HealthCheckProtocol": "TCP", "HealthCheckPort": "80", "TargetType": "instance"}}, "Listener": {"Type": "AWS::ElasticLoadBalancingV2::Listener", "Properties": {"DefaultActions": [{"Type": "forward", "TargetGroupArn": {"Ref": "TargetGroup"}}], "LoadBalancerArn": {"Ref": "NLB"}, "Port": 80, "Protocol": "TCP"}}}, "Parameters": {"Subnet1": {"Type": "AWS::EC2::Subnet::Id", "Description": "The ID of the first subnet to associate with the NLB."}, "Subnet2": {"Type": "AWS::EC2::Subnet::Id", "Description": "The ID of the second subnet to associate with the NLB."}, "VPC": {"Type": "AWS::EC2::VPC::Id", "Description": "The ID of the VPC."}}, "Outputs": {"NLBArn": {"Description": "The ARN of the Network Load Balancer", "Value": {"Ref": "NLB"}}}}