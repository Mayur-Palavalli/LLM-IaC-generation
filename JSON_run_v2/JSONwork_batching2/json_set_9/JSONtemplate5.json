{"AWSTemplateFormatVersion": "2010-09-09", "Resources": {"VPC": {"Type": "AWS::EC2::VPC", "Properties": {"CidrBlock": "10.0.0.0/16", "EnableDnsSupport": true, "EnableDnsHostnames": true, "Tags": [{"Key": "Name", "Value": "PrivateLinkVPC"}]}}, "Subnet": {"Type": "AWS::EC2::Subnet", "Properties": {"VpcId": {"Ref": "VPC"}, "CidrBlock": "10.0.1.0/24", "AvailabilityZone": {"Fn::Select": [0, {"Fn::GetAZs": ""}]}, "Tags": [{"Key": "Name", "Value": "PrivateLinkSubnet"}]}}, "VPCEndpointService": {"Type": "AWS::EC2::VPCEndpointService", "Properties": {"AcceptanceRequired": true, "NetworkLoadBalancerArns": ["arn:aws:elasticloadbalancing:region:account-id:loadbalancer/net/load-balancer-id"]}}, "Endpoint": {"Type": "AWS::EC2::VPCEndpoint", "Properties": {"ServiceName": {"Fn::Sub": "com.amazonaws.vpce.${AWS::Region}.vpce-svc-0123456789abcdef"}, "VpcId": {"Ref": "VPC"}, "VpcEndpointType": "Interface", "PrivateDnsEnabled": true, "SubnetIds": [{"Ref": "Subnet"}], "SecurityGroupIds": [{"Ref": "VPCEndpointSecurityGroup"}]}}, "VPCEndpointSecurityGroup": {"Type": "AWS::EC2::SecurityGroup", "Properties": {"GroupDescription": "Security group for VPC Endpoint", "VpcId": {"Ref": "VPC"}, "SecurityGroupIngress": [{"IpProtocol": "tcp", "FromPort": 443, "ToPort": 443, "CidrIp": "0.0.0.0/0"}]}}}, "Outputs": {"VPCEndpointID": {"Description": "VPC Endpoint ID", "Value": {"Ref": "Endpoint"}}, "VPCEndpointServiceId": {"Description": "VPC Endpoint Service ID", "Value": {"Fn::GetAtt": ["VPCEndpointService", "ServiceId"]}}}}